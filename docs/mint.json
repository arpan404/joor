{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Joor",
  "theme": "prism",
  "logo": {
    "dark": "/logo/dark-transparent.png",
    "light": "/logo/light-transparent.png"
  },
  "layout": "sidenav",
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#2E77BB",
    "light": "#6BA4E8",
    "dark": "#1A5D9F",
    "anchors": {
      "from": "#2E77BB",
      "to": "#90C6FF"
    }
  },
  "topbarCtaButton": {
    "type": "github",
    "url": "https://github.com/arpan404/joor"
  },
  "tabs": [
    {
      "name": "References",
      "url": "reference"
    }
  ],
  "anchors": [
    {
      "name": "NPM",
      "icon": "npm",
      "url": "https://www.npmjs.com/package/joor"
    },

    {
      "name": "Github",
      "icon": "github",
      "url": "https://mintlify.com/docs"
    },
    {
      "name": "Discord",
      "icon": "discord",
      "url": "https://mintlify.com/community"
    }
  ],
  "sidebar": {
    "items": "border"
  },
  "navigation": [
    {
      "group": "Getting Started",
      "version": "v1",
      "pages": ["v1/introduction", "v1/quickstart", "v1/development"]
    },
    {
      "group": "Essentials",
      "pages": [
        "essentials/markdown",
        "essentials/code",
        "essentials/images",
        "essentials/settings",
        "essentials/navigation",
        "essentials/reusable-snippets"
      ]
    },
    {
      "group": "Reference",
      "pages": ["reference/cors"]
    }
  ],
  "feedback": {
    "raiseIssue": true,
    "suggestEdit": true,
    "thumbsRating": true
  },
  "footerSocials": {
    "x": "https://x.com/joorofficial",
    "github": "https://github.com/arpan404/joor"
  },

  "versions": [
    {
      "name": "v1",
      "default": true
    }
  ]
}